{
    "version": 2,
    "builds": [
        {
            "src": "./main.ts",
            "use": "@vercel/node",
            "config": {
        "includeFiles": [
          "prisma/schema.prisma",
          "node_modules/.prisma/client/**"
        ]
      }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}